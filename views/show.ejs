<% include ./partials/header.ejs %>
    <div class="w-full min-h-screen bg-zinc-700 px-10 py-8 text-white ">
        <div class="cards flex flex-wrap gap-10">
            <%user.posts.forEach(function(post){ %>
                <div class="card w-45 ">
                    <div class=" w-full h-45 bg-sky-200 rounded-lg overflow-hidden">
                        <img src="/images/uploads/<%=post.image%>" class="w-full h-full object-cover" alt="">
                    </div>
                    <h5 class="mt-2 text-xl capitalize"><%=post.title%></h5>
                    <h5 class="text-xs capitalize opacity-80"><%=post.description %></h5>
                </div>
                <% })%>
        </div>
    </div>
    <% include ./partials/footer.ejs %>